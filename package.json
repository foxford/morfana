{
  "name": "@foxford/morfana",
  "version": "1.0.0",
  "description": "Morphemes higlighting library",
  "bugs": {
    "url": "https://github.com/netology-group/morfana/issues"
  },
  "repository": {
    "url": "https://github.com/netology-group/morfana"
  },
  "license": "MIT",
  "author": "OLC Foxford LLC",
  "contributors": [
    "Natalia Chigryay <n.chigryay@netology-group.ru>"
  ],
  "files": [
    "es"
  ],
  "main": "es/index.js",
  "module": "es/index.js",
  "directories": {
    "lib": "es"
  },
  "exports": {
    ".": {
      "require": "./es/index.js",
      "default": "./es/index.js"
    },
    "./package.json": "./package.json"
  },
  "typings": "es/index.d.ts",
  "scripts": {
    "babel": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "prebuild": "rm -rf es && mkdir -p es",
    "build": "cross-env NODE_ENV=production npm run build:es && npm run build:types",
    "build:es": "cross-env BABEL_ENV=production npm run babel -- -d es",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir es",
    "precoverage": "rm -rf coverage",
    "coverage": "npm run jest -- --coverage --ci",
    "coverage/report": "npm run jest && codecov --disable=gcov",
    "lint": "eslint --ext .jsx,.js,.ts -c .eslintrc.js ./src",
    "start": "npm run storybook",
    "test": "NODE_ENV=test BABEL_ENV=test jest",
    "type-checking": "tsc --rootDir ./src --noEmit --pretty",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": true,
  "dependencies": {
    "react": ">=16 <=18",
    "react-dom": ">=16 <=18",
    "styled-components": ">=5.x"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.3",
    "@babel/eslint-parser": "7.21.3",
    "@babel/preset-typescript": "7.21.0",
    "@babel/register": "7.21.0",
    "@foxford/babel-preset-react-app": "1.3.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/react": "6.5.16",
    "@types/jest": "29.5.0",
    "@types/react": "18.0.28",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "babel-plugin-jsx-control-statements": "4.1.2",
    "babel-plugin-styled-components": "2.0.7",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "codecov": "3.8.2",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsdoc": "40.0.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-jsx-control-statements": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-unicorn": "46.0.0",
    "eslint-restricted-globals": "0.2.0",
    "jest": "29.5.0",
    "jest-cli": "29.5.0",
    "jest-runner": "29.5.0",
    "prettier": "2.8.4",
    "ts-node": "10.9.1",
    "typescript": "5.0.2",
    "zx": "7.0.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9.x"
  },
  "peerDependencies": {
    "react": ">=16 <=18",
    "react-dom": ">=16 <=18",
    "styled-components": ">=5.x"
  }
}
